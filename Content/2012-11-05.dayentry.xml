      <Content>&lt;div id="scid:9ce6104f-a9aa-4a17-a79f-3a39532ebf7c:5ae1a476-0471-4e77-9633-bf23c7cb99a3" class="wlWriterEditableSmartContent" style="float: none; padding-bottom: 0px; padding-top: 0px; padding-left: 0px; margin: 0px; display: inline; padding-right: 0px"&gt; &lt;div style="border: #000080 1px solid; color: #000; font-family: 'Courier New', Courier, Monospace; font-size: 10pt"&gt; &lt;div style="background-color: #ffffff; overflow: auto; padding: 2px 5px; white-space: nowrap"&gt;&lt;span style="background:#22282a;color:#f1f2f3"&gt; &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;// Suppress Silverlight right-click menu for clicks on ListBoxItem elements&lt;/span&gt;&lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;//AccountItemsT32.MouseRightButtonDown += delegate(object sender, MouseButtonEventArgs e)&lt;/span&gt;&lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;//{&lt;/span&gt;&lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;//    e.Handled = true;&lt;/span&gt;&lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;//};&lt;/span&gt;&lt;br&gt; &lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#66747b"&gt;// Add the Mouse Right click Event Handler to disable all S/L right click.&lt;/span&gt;&lt;br&gt; &lt;span style="background:#22282a;color:#f1f2f3"&gt;            &lt;/span&gt;&lt;span style="background:#22282a;color:#93c763"&gt;this&lt;/span&gt;&lt;span style="background:#22282a;color:#e8e2b7"&gt;.&lt;/span&gt;&lt;span style="background:#22282a;color:#f1f2f3"&gt;MouseRightButtonDown &lt;/span&gt;