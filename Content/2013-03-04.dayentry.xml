e1e;color:#dcdcdc"&gt;(&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#4ec9b0"&gt;QueryDescription&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt; &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;queryDescription&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt;, &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#569cd6"&gt;out&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt; &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b8d7a3"&gt;IEnumerable&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b4b4b4"&gt;&amp;lt;&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;System&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b4b4b4"&gt;.&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;ComponentModel&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b4b4b4"&gt;.&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;DataAnnotations&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b4b4b4"&gt;.&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#4ec9b0"&gt;ValidationResult&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#b4b4b4"&gt;&amp;gt;&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt; &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;validationErrors&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt;, &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#569cd6"&gt;out&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt; &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#569cd6"&gt;int&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt; &lt;/span&gt;&lt;span style="background:#1e1e1e;color:#ffffff"&gt;totalCount&lt;/span&gt;&lt;span style="background:#1e1e1e;color:#dcdcdc"&gt;)&lt;/span&gt;&lt;br&gt; &lt;span style="background:#1e1e1e;color:#dcdcdc"&gt;        {&lt;/span&gt;&lt;br&gt; &lt;span style="background:#1e1e1e;color:#9b9b9b"&gt;      